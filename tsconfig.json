{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "feature1": [
        "dist/feature1"
      ],
      "feature1/*": [
        "dist/feature1/*"
      ],
      "ex-lib2": [
        "dist/ex-lib2"
      ],
      "ex-lib2/*": [
        "dist/ex-lib2/*"
      ],
      "noob-comm": [
        "dist/noob-comm"
      ],
      "noob-comm/*": [
        "dist/noob-comm/*"
      ],
      "ngbootstrap-carousel-extended": [
        "dist/ngbootstrap-carousel-extended"
      ],
      "ngbootstrap-carousel-extended/*": [
        "dist/ngbootstrap-carousel-extended/*"
      ]
    }
  }
}